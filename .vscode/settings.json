{
  "remote.antigravityDevContainers.experimental.disableServerChecksum": true,
  "remote.antigravitySSH.experimental.disableServerChecksum": true,
  "editor.fontFamily": "VT323, Courier Prime Code, Cascadia Code, Source Code Pro, IBM Plex Mono, Consolas, Fira Code, JetBrains Mono",
  "editor.fontLigatures": true,
  "editor.fontSize": 15,
  "editor.wordWrap": "on",
  "editor.renderWhitespace": "none",
  "editor.cursorSmoothCaretAnimation": "on",
  "editor.formatOnSave": true,
  "editor.formatOnPaste": true,
  "editor.codeActionsOnSave": {
    "source.fixAll": "explicit",
    "source.organizeImports": "explicit"
  },
  "editor.tabSize": 2,
  "editor.insertSpaces": true,
  "editor.detectIndentation": false,
  "editor.minimap.enabled": false,
  "editor.scrollBeyondLastLine": false,
  "editor.smoothScrolling": true,
  "files.autoSave": "afterDelay",
  "files.trimTrailingWhitespace": true,
  "files.insertFinalNewline": true,
  "files.exclude": {
    "**/node_modules": true,
    "**/.git": true,
    "**/playwright-report": true,
    "**/test-results": true,
    "**/dist": true
  },
  "workbench.productIconTheme": "fluent-icons",
  "workbench.startupEditor": "none",
  "workbench.sideBar.location": "left",
  "symbols.hidesExplorerArrows": false,
  "breadcrumbs.enabled": true,
  "terminal.integrated.fontSize": 14,
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.defaultProfile.windows": "Git Bash",
  "git.autofetch": true,
  "git.confirmSync": false,
  "git.enableSmartCommit": true,
  "search.exclude": {
    "**/node_modules": true,
    "**/playwright-report": true,
    "**/test-results": true
  },
  "telemetry.telemetryLevel": "off",
  "update.mode": "manual",
  "extensions.autoUpdate": false,
  "editor.guides.bracketPairs": true,
  "todo-tree.general.tagGroups": {},
  "todo-tree.general.tags": [
    "BUG",
    "DEFECT",
    "ERROR",
    "CRITICAL",
    "BLOCKER",
    "FIX",
    "FIXME",
    "HOTFIX",
    "REFACTOR",
    "REWORK",
    "CLEANUP",
    "TODO",
    "TASK",
    "NOTE",
    "INFO",
    "DOC",
    "DOCUMENT",
    "TEST",
    "TESTING",
    "ASSERT",
    "ASSERTION",
    "FLAKY",
    "RETRY",
    "SKIP",
    "PERF",
    "PERFORMANCE",
    "OPTIMIZE",
    "SECURITY",
    "VULN",
    "RISK",
    "HACK",
    "WORKAROUND",
    "TEMP",
    "TEMPORARY",
    "REMOVE",
    "XXX",
    "IMPORTANT",
    "ATTENTION",
    "REVIEW",
    "CHECK",
    "[ ]",
    "[x]",
    "[X]",
    "todo",
    "fixme",
    "bug"
  ],
  "bracketPairColorizer.showBracketsInGutter": true,
  "bracketPairColorizer.showBracketsInRuler": true,
  "bracketPairColorizer.highlightActiveScope": true,
  "bracketPairColorizer.forceIterationColorCycle": true,
  "explorer.fileNesting.patterns": {
    "*.ts": "${capture}.js",
    "*.js": "${capture}.js.map, ${capture}.min.js, ${capture}.d.ts",
    "*.jsx": "${capture}.js",
    "*.tsx": "${capture}.ts",
    "tsconfig.json": "tsconfig.*.json",
    "package.json": "package-lock.json, yarn.lock, pnpm-lock.yaml, bun.lockb, bun.lock",
    "*.sqlite": "${capture}.${extname}-*",
    "*.db": "${capture}.${extname}-*",
    "*.sqlite3": "${capture}.${extname}-*",
    "*.db3": "${capture}.${extname}-*",
    "*.sdb": "${capture}.${extname}-*",
    "*.s3db": "${capture}.${extname}-*"
  },
  "vscode-office.editorTheme": "Light",
  "workbench.colorTheme": "Tokyo Night AltSynt",
  "explorer.confirmDelete": false,
  "sherlock.userId": "cec1c5cf-e19e-4452-92d6-73aace2bfc4b",
  "geminicodeassist.project": "integrated-keel-0xmd0",
  "editor.codeActions.triggerOnFocusChange": true,
  "[typescriptreact]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "bracketPairColorizer.depreciation-notice": false,
  "snyk.advanced.cliPath": "C:\\Users\\Raihan Khan\\AppData\\Local\\snyk\\vscode-cli\\snyk-win.exe",
  "snyk.securityAtInception.autoConfigureSnykMcpServer": true,
  "snyk.securityAtInception.executionFrequency": "On Code Generation",
  "snyk.yesWelcomeNotification": false,
  "editor.fontWeight": "normal",
  "editor.fontVariations": false,
  "snyk.advanced.autoSelectOrganization": true
}
